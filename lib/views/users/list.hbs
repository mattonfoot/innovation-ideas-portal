<div class="container">
  <div class="page-header">
    <h1>Users</h1>
  </div>
</div>

<div class="container">
  <h2>Active users</h2>

  <div class="row">
    {{#each users as |profile|}}
    <div class="col-sm-6">
      <a href="/users/{{ profile._id }}" class="btn btn-default">{{ profile.username }}</a>
    </div>
    {{/each}}
  </div>
</div>
