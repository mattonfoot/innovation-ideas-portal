<div class="container">
  <div class="page-header">
    <h1>{{ user.username }}</h1>
  </div>
</div>

<div class="container">
  <h2>Projects</h2>

  <div class="row grid">
    {{#each projects as |project|}}
    <div class="col-sm-12 col-md-3 grid-item">
      {{> projectTile project }}
    </div>
    {{/each}}
  </div>
</div>
